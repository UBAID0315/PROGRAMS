#include <stdio.h>
int main()
{
    int n1, n2, i, gcd;

    printf("Enter two integers: ");
    scanf("%d %d", &n1, &n2);

	for(int i=1;i<=n1 && i<=n2;++i)
	{
		if(n1%i==0 && n2%i==0)              //at which number n1 and n2 become zero that's be the HCF values
		{
			gcd = i;
		}
	}

    printf("G.C.D of %d and %d is %d", n1, n2, gcd);

    return 0;
}
