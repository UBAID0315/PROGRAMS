#include<stdio.h>
#include<stdlib.h>
#include<time.h>

void main()
{
    srand(time(NULL));                                           // to get random no every time

    int lottery[5],count=0;
    int user[5];
                                                                // random lottery no generated by program
    for (int i = 0; i < 5; i++)
    {
        lottery[i]=rand() % 9;
    }
                                                                // user lottery number
    printf("Enter your lottery number btw 0 to 5: ");
    
    for (int i = 0; i < 5; i++)
    {
        scanf("%d", &user[i]);
    }
    printf("The match numbers are:");                         // comparsion of user and lottery no
    for (int i = 0; i < 5; i++)
    {
        if (user[i] == lottery[i])
        {
            printf(" %d",user[i]);
            count++;
        }
        else
        {
            continue;
        }
    }
                                                                // for print lottery no  
    printf("\nThe number of lottery is: ");
    for (int  i = 0; i < 5; i++)
    {
        printf("%d ",lottery[i]);
    }
    printf("\n");
                                                                // check if all no are match   
    if (count == 5)
    {
        printf("congrats! you win the grand price");
    }
    
}
